# Staging environment configuration
ENV=staging

# Application URLs
BASE_URL=https://staging.example.com
API_URL=https://api.staging.example.com

# Browser settings
BROWSER=chromium
HEADLESS=true
SLOW_MO=0

# Timeouts (ms)
DEFAULT_TIMEOUT=30000
API_TIMEOUT=15000

# Auth test data
TEST_USER_EMAIL=
TEST_USER_PASSWORD=Password123
TEST_USER_FIRST_NAME=Demo
TEST_USER_LAST_NAME=User
TEST_USER_DATE_OF_BIRTH=01.08.2001

# Auth endpoints / token mapping
AUTH_LOGIN_PATH=/api/public/login
AUTH_REGISTER_PATH=/api/public/registration
AUTH_TOKEN_FIELD=jwt-token

# Logging
LOG_SENSITIVE=false

# Parallel execution
WORKERS=4
